[environment]
kernel = include/sample_vm/ttylinux-vmlinuz
initrd = include/sample_vm/ttylinux-initrd
image = include/sample_vm/ttylinux.img

[swift]
auth_host = <%= @ip_address %>
auth_port = <%= @swift_auth_port %>
auth_prefix = <%= @swift_auth_prefix %>
auth_ssl = <%= @swift_ssl %>
account = <%= @swift_account %>
username = <%= @swift_user %>
password = <%= @swift_pass %>

[rabbitmq]
host = <%= @ip_address %>
user = <%= @rabbit_user %>
password = 

[nova]
network_label = <%= @nova_network_label %>

[keystone]
host = <%= @ip_address %>
port = <%= @keystone_service_port %>
admin_port = <%= @keystone_admin_port %>
apiver = <%= @keystone_apiver %>
user = <%= @keystone_user %>
password = <%= @keystone_pass %>
tenantname = <%= @keystone_tenantid %>
region = <%= @keystone_region %>

[KongRequester]
auth_url = http://<%= @ip_address %>:<%= @keystone_service_port %>
user = <%= @keystone_user %>
password = <%= @keystone_pass %>
tenantname = <%= @keystone_tenantid %>
region = <%= @keystone_region %>
