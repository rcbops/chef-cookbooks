[environment]
kernel = include/sample_vm/ttylinux-vmlinuz
initrd = include/sample_vm/ttylinux-initrd
image = include/sample_vm/ttylinux.img

[swift]
auth_host = <%= @ip_address %>
auth_port = <%= @swift_auth_port %>
auth_prefix = <%= @swift_auth_prefix %>
auth_ssl = <%= @swift_ssl %>
account = <%= @swift_account %>
username = <%= @swift_user %>
password = <%= @swift_pass %>
auth_type = swauth
